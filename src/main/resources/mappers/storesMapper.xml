<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.quiz.store.dao.StoresMapper">
	<select id="selectStoresList" resultType="com.quiz.store.domain.Stores">
		SELECT
			name
			phoneNumber
			address
		FROM
			`store`
	</select>
</mapper>